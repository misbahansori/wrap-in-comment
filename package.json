{
  "name": "wrap-in-comment",
  "publisher": "misbahansori",
  "displayName": "wrap-in-comment",
  "description": "Wrap code inside comments",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/misbahansori/wrap-in-comment"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wrap-in-comment.wrap-in-comment",
        "title": "Wrap in comment"
      }
    ],
    "keybindings": [
      {
        "command": "wrap-in-comment.wrap-in-comment",
        "key": "ctrl+k ctrl+/",
        "mac": "cmd+k cmd+/"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
